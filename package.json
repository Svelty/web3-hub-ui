    "ethers": "^5.6.9",
    "pinia": "^2.0.16",
    "react-redux": "^7.2.8",
    "veaury": "^2.2.10",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "web3": "^1.7.4"
